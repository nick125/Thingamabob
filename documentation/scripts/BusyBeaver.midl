DEFINE A
	IF 0
		WRITE 1
		MOVE RIGHT
		GOTO B
	ENDIF
	IF 1
		WRITE 1
		MOVE RIGHT
		HALT
	ENDIF
ENDDEFINE
DEFINE B
	IF 0
		WRITE 0
		MOVE RIGHT
		GOTO C
	ENDIF
	IF 1
		WRITE 1
		MOVE RIGHT
		GOTO B
	ENDIF
ENDDEFINE
DEFINE C
	IF 0
		WRITE 1
		MOVE LEFT
		GOTO C
	ENDIF
	IF 1
		WRITE 1
		MOVE LEFT
		GOTO A
	ENDIF
ENDDEFINE